[ignore]
.*/node_modules/oboe/*
.*/packages/loan-contract/node_modules/*
[include]
./src/
[libs]

[lints]

[options]
module.name_mapper='^react-native$' -> 'react-native-web'
module.name_mapper='^pages\(.*\)$' -> '<PROJECT_ROOT>/src/pages/\1'
module.name_mapper='^services\(.*\)$' -> '<PROJECT_ROOT>/src/services/\1'
module.name_mapper='^ui\(.*\)$' -> '<PROJECT_ROOT>/src/ui/\1'
module.name_mapper='^abi$' -> '<PROJECT_ROOT>/src/abi.js'
module.name_mapper='^App$' -> '<PROJECT_ROOT>/src/App.js'
module.name_mapper='^index' -> '<PROJECT_ROOT>/src/index.js'
module.name_mapper='^styles' -> '<PROJECT_ROOT>/src/styles.js'
module.name_mapper='^theme' -> '<PROJECT_ROOT>/src/theme.js'
module.name_mapper='^types' -> '<PROJECT_ROOT>/src/types.js'

[strict]
